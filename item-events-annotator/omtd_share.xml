<componentMetadataRecord xmlns="http://www.meta-share.org/OMTD-SHARE_XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.meta-share.org/OMTD-SHARE_XMLSchema http://www.meta-share.org/OMTD-SHARE_XMLSchema/v302/OMTD-SHARE-Component.xsd">
  <componentInfo>
    <resourceType>component</resourceType>
    <identificationInfo>
      <resourceNames>
        <resourceName>Item and Events Annotator</resourceName>
      </resourceNames>
      <descriptions>
        <description>Annotates sentences, items and events. The items to be annotated are located in a dictionary file. The default dictionary is a list of liver diseases (including synonyms). As for the events, they are located in a list file. The default list contains progression keywords.</description>
      </descriptions>
      <resourceIdentifiers>
        <resourceIdentifier resourceIdentifierSchemeName="OMTD-docker">com.github.ewoij.omtd.item_event_annotator</resourceIdentifier>
      </resourceIdentifiers>
      <public>false</public>
    </identificationInfo>
    <versionInfo>
      <version>3.0.0</version>
    </versionInfo>
    <contactInfo>
      <contactPoint>jonas.berdoz@gmail.com</contactPoint>
      <contactType>contactEmail</contactType>
    </contactInfo>
    <application>true</application>
    <functionInfo>
      <function>http://w3id.org/meta-share/omtd-share/Annotator</function>
    </functionInfo>
    <distributionInfos>
      <componentDistributionInfo>
        <componentDistributionForm>dockerImage</componentDistributionForm>
        <distributionLocation>ewoij/item-event-annotator:3.0.0</distributionLocation>
        <command>annotates</command>
      </componentDistributionInfo>
    </distributionInfos>
    <rightsInfo>
      <licenceInfos>
        <licenceInfo>
          <licence>Apache-2.0</licence>
        </licenceInfo>
      </licenceInfos>
      <rightsStatement>openAccess</rightsStatement>
    </rightsInfo>
    <inputContentResourceInfo>
      <processingResourceType>corpus</processingResourceType>
      <dataFormats>
        <dataFormatInfo>
          <dataFormat>http://w3id.org/meta-share/omtd-share/Xmi</dataFormat>
        </dataFormatInfo>
      </dataFormats>
      <languages>
        <language>en</language>
      </languages>
    </inputContentResourceInfo>
    <outputResourceInfo>
      <processingResourceType>corpus</processingResourceType>
      <dataFormats>
        <dataFormatInfo>
          <dataFormat>http://w3id.org/meta-share/omtd-share/Xmi</dataFormat>
        </dataFormatInfo>
      </dataFormats>
    </outputResourceInfo>
    <componentCreationInfo>
      <framework>UIMA</framework>
      <implementationLanguage>Java</implementationLanguage>
    </componentCreationInfo>
  </componentInfo>
</componentMetadataRecord>